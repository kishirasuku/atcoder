def main():
    m=input()
    
    if m<100:
        print 00
        return
    elif 101<m<5000:
        print "%02d" % int(float(m/1000)*10)
        return

    m=m/1000
    if 6<=m<=30:
        print m+50
    elif 35<=m<=70:
        print ((m-30)/5)+80
    else:
        print 89
        return

if __name__ == '__main__':
    main()
        
        
        
        
